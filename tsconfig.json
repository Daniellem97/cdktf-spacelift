{
  "compilerOptions": {
    "outDir": "dist",
    "module": "CommonJS",
    "target": "ES2018",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "inlineSourceMap": true,
    "inlineSources": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "cdktf.out"]
}
